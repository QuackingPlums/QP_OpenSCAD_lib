		translate([-metricsL/2, -lineHeight/2, 0])
		doText(text, size=size, spacing=spacing, align=align);
	} else if (align == "R") {
		translate([-metricsL, -lineHeight/2, 0])
		doText(text, size=size, spacing=spacing, align=align);
	} else {
		translate([0, -lineHeight/2, 0])
		doText(text, size=size, spacing=spacing, align=align);
	}
}

