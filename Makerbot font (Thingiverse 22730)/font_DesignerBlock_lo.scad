	(e-s < 0) ? 0 :

	  ((e-s >= 0) ? metrics_single(t[s]) : 0)
	+ ((e-s >= 1) ? metrics_single(t[s+1]) : 0)
	+ ((e-s >= 2) ? metrics_single(t[s+2]) : 0)
	+ ((e-s >= 3) ? metrics_single(t[s+3]) : 0)
	+ ((e-s >= 4) ? metrics_single(t[s+4]) : 0)
