	+ ((e-s >= 6) ? metrics_single(t[s+6]) : 0)
	+ ((e-s >= 7) ? metrics_single(t[s+7]) : 0)
	+ ((e-s >= 8) ? metrics(t, e, s+8) : 0)
;


function label_width(text, len=-1, size=1.0, spacing=1.0) =
	(metrics(text, len(text)-1)
	* (len(text) < 2 ? 1 : spacing))
	* size;

module doText(text, size=4.2, spacing=1.0) {
	len = len(text);
	scale(size)
	for (c = [0:len-1]) {
		translate([metrics(text, c-1) * spacing, 0, 0])
		doChar(text[c]);
	}
}

module label(text, size=5.0, height=1.0, spacing=1.0, align="C") {
	metricsL = label_width(text, len(text), size, spacing);
	lineHeight = 0;
	linear_extrude(height=height)
	if (align == "C") {
		translate([-metricsL/2, -lineHeight/2, 0])
		doText(text, size=size, spacing=spacing, align=align);
	} else if (align == "R") {
		translate([-metricsL, -lineHeight/2, 0])
		doText(text, size=size, spacing=spacing, align=align);
	} else {
		translate([0, -lineHeight/2, 0])
		doText(text, size=size, spacing=spacing, align=align);
	}
}

